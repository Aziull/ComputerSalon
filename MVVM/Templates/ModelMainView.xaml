                     xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:vm="clr-namespace:MVVM.ViewModels"
                    xmlns:view="clr-namespace:MVVM.Pages">
    
    <DataTemplate DataType="{x:Type vm:ConfigPageViewModel}">
        <view:ConfigPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ExistingUnitsPageViewModel}">
        <view:ExistingUnitsPage  />
    </DataTemplate>
    
</ResourceDictionary>