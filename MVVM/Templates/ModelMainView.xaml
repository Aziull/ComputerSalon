<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:vm="clr-namespace:MVVM.ViewModels"
                    xmlns:view="clr-namespace:MVVM.Pages">
    <DataTemplate DataType="{x:Type system:Int32}">
        <TextBlock Text="{Binding StringFormat=int:{0}}" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ConfigViewModel}">
        <view:ConfigPage DataContext="{Binding}" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ExistingUnitsViewModel}">
        <view:ExistingUnitsPage DataContext="{Binding}" />
    </DataTemplate>
    
</ResourceDictionary>